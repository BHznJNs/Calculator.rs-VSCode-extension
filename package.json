{
  "name": "calculator-rs-support",
  "displayName": "Calculator.rs support",
  "description": "VSCode extension for Calculator.rs",
  "version": "1.2.0",
  "publisher": "BHznJNs",
  "license": "MIT",
  "repository": "https://github.com/BHznJNs/Calculator.rs-VSCode-extension",
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "contributes": {
    "languages": [{
      "id": "calcrs",
      "aliases": ["Calculator.rs", "calcrs"],
      "extensions": [".calcrs"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "calcrs",
      "scopeName": "source.calcrs",
      "path": "./syntaxes/calcrs.tmLanguage.json"
    }],
    "snippets": [{
      "language": "calcrs",
      "path": "./snippets/calcrs.snippets.json"
    }]
  }
}
