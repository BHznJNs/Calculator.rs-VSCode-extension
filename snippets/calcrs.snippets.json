{
    "For loop": {
        "prefix": "for",
        "body": [
            "for ${1:Loop Count} {",
            "    ${2:Loop Body}",
            "}"
        ]
    },
    "If condition": {
        "prefix": "if",
        "body": [
            "if ${1:Condition} {",
            "    ${2:Condition Body}",
            "}"
        ]
    },
    "Function definition": {
        "prefix": "fn",
        "body": [
            "${1:Function Name} = fn(${2: Function Params}) {",
            "    ${3:Function Body}",
            "}"
        ]
    },
    "Class definition": {
        "prefix": "cl",
        "body": [
            "${1:Class Name} = cl {",
            "    ${2:Class Props and Methods}",
            "}"
        ]
    }
}