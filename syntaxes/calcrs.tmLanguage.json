{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Calculator.rs",
	"patterns": [
		{"include": "#functions"},
		{"include": "#variable"},
		{"include": "#numbers"},
		{"include": "#comments"}
	],
	"repository": {
		"functions": {
			"patterns": [{
				"name": "entity.name.function.calcrs",
				"match": "([_a-zA-Z][_a-zA-Z0-9]*)(?=\\()"
			}]
		},
		"variable": {
			"patterns": [{
				"name": "variable.calcrs",
				"match": "[_a-zA-Z][_a-zA-Z0-9]*"
			}]
		},
		"numbers": {
			"patterns": [
				{
					"name": "constant.numeric.float.calcrs",
					"match": "^(-?\\d+)(\\.\\d+)?$"
				},
				{
					"name": "constant.numeric.integer.calcrs",
					"match": "-?[0-9]+"
				}
			]
		},
		"comments": {
			"patterns": [{
				"name": "comment.line.calcrs",
				"match": "#.*$"
			}]
		}
	},
	"scopeName": "source.calcrs"
}